<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/getDogsList"
    tools:context="com.example.jq.app_code.getHumans">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentRight="true"
        android:orientation="vertical"
        android:id="@+id/topPaneLayout">

        <Button
            android:id="@+id/getPostsButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/getList" />

    </LinearLayout>


    <ListView
        android:id="@+id/dogsListView"
        android:layout_width="match_parent"
        android:layout_height="450dp"
        android:layout_below="@+id/topPaneLayout">
    </ListView>


</RelativeLayout>

